        "watch:webpack": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 -r tsconfig-paths/register\" webpack --mode=development --watch --config webpack.dev.ts",
        "watch:prod:webpack": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 -r tsconfig-paths/register\" webpack --mode=production --watch --config webpack.prod.ts",
        "start:pwa": "http-server dist",
        "start:webpack:devserver": "webpack-dev-server --open --config webpack.dev.ts",
        "start:webpack:devserver:prod": "webpack-dev-server --open --config webpack.prod.ts",
        "watch": "",
        "watch:dotnet": "dotnet watch run",
        "inst": "npm install",
        "inst:force": "npm install --force",
        "uninst": "npm uninstall",
        "init": "npm install -D concurrently npm-run-all",
        "nuke": "rimraf node_modules",
        "reset": "npm run nuke && npm run init && npm run inst",
        "generate:paths": "ts-node -r tsconfig-paths/register ./scripts/generate-paths.ts"
    },
