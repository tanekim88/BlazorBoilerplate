        "test:e2e": "jest --config ./test/jest-e2e.json",
        "lint": "cross-env NODE_OPTIONS=--max_old_space_size=4096 eslint **/*.ts --fix --no-eslintrc -c .eslintrc.json",
        "build:webpack": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 -r tsconfig-paths/register\" webpack --mode=development --config webpack.dev.ts",
